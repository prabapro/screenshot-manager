{
  "name": "screenshot-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:viz": "pnpm dlx vite-bundle-visualizer --report dist/stats.json --output dist/bundle-visualizer.html",
    "build": "vite build",
    "deploy:pages": "npm run build && wrangler pages deploy ./dist",
    "dev:env": "infisical init && infisical export --env=dev --format=dotenv --path=/ > .dev.vars && echo \"âœ… .dev.vars generated. Please make sure it is added to .gitignore\"",
    "dev": "concurrently \"vite\" -c \"green,blue\" \"wrangler pages dev\"",
    "format": "prettier --write \"./**/*.{html,js,jsx,json,css,ts,tsx}\"",
    "generate-jsconfig": "node tools/generate-jsconfig.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "update-tree": "node tools/update-tree.js",
    "update-version": "node tools/update-version.js && pnpm install && pnpm format && pnpm update-tree"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ora": "^8.2.0",
    "prettier": "^3.5.3",
    "semver": "^7.7.2",
    "terser": "^5.39.2",
    "tw-animate-css": "^1.3.0",
    "vite": "^6.3.5",
    "vite-plugin-minify": "^2.1.0",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
